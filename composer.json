{
    "name": "danepowell/mutation-example",
    "description": "PHP mutation testing example",
    "require": {
        "php": "^8",
        "ext-pcov": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "infection/infection": "^0.26.19",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "Danepowell\\MutationExample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danepowell\\MutationExample\\Tests\\": "tests/src"
        }
    },
    "authors": [
        {
            "name": "Dane Powell",
            "email": "git@danepowell.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        },
        "platform": {
            "php": "8.1"
        }
    }
}
